{
  "expo": {
    "name": "Shift",
    "slug": "Shift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "Shift",
    "splash": {
      "image": "./assets//icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C12B2B"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#C12B2B"
      }
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
